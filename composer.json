{
    "name": "marcosraudkett/tallink",
    "description": "Tallink PHP Client",
    "homepage": "https://github.com/marcosraudkett/tallink",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Marcosraudkett\\Tallink\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marcos Raudkett",
            "email": "info@marcosraudkett.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": true
    },
    "require": {},
    "require-dev": {
        "pestphp/pest": "^1.20"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
